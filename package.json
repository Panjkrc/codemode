{
  "name": "turborepo-basic-shared",
  "version": "0.0.0",
  "private": true,
  "workspaces": [
    "apps/*",
    "packages/*"
  ],
  "scripts": {
    "build": "turbo run build",
    "dev": "turbo run dev --parallel",
    "lint": "turbo run lint",
    "test": "turbo run test:unit",
    "format": "prettier --write \"**/*.{ts,tsx,md,js,vue}\""
  },
  "devDependencies": {
    "prettier": "^2.5.1",
    "turbo": "^1.1.10"
  },
  "engines": {
    "npm": ">=7.0.0",
    "node": ">=14.0.0"
  },
  "packageManager": "yarn@1.22.10"
}
